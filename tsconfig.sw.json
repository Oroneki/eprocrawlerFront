{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noEmit": false,
    "jsx": "preserve",
    "rootDirs": [
      "sw_src"
    ],
    "outDir": "public/sw"
  },
  "files": [
    "sw_src/worker.ts",
  ]
}